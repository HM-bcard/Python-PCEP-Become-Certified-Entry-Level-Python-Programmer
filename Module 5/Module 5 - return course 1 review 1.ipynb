   "source": [
    "input_numbers = [5., 3.5, 7.8, 9.9]\n",
    "\n",
    "def get_average(input_numbers):\n",
    "    sum = 0.0\n",
    "    for number in input_numbers:\n",
    "        sum += number\n",
    "    average = sum / len(input_numbers)\n",
    "    return(average)"
   ]
